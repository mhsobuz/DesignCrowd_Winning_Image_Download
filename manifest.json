{
  "manifest_version": 3,
  "name": "DesignCrowd Contest Images Downloader",
  "version": "1.0",
  "description": "Download all HD images from DesignCrowd contests",
  "permissions": ["downloads", "scripting", "activeTab"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Download Contest Images",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
